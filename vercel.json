{
  "version": 2,
  "installCommand": "npm install --legacy-peer-deps",
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build", "config": { "distDir": "build" } }
  ],
  "env": {
    "SKIP_PREFLIGHT_CHECK": "true"
  },
  "routes": [
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}


